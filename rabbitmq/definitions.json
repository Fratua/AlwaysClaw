{
  "rabbit_version": "3.12",
  "vhosts": [
    {
      "name": "/"
    }
  ],
  "queues": [
    {
      "name": "agent.tasks",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {}
    },
    {
      "name": "agent.results",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {}
    },
    {
      "name": "agent.events",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {}
    }
  ],
  "exchanges": [
    {
      "name": "openclaw",
      "vhost": "/",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    }
  ],
  "bindings": [
    {
      "source": "openclaw",
      "vhost": "/",
      "destination": "agent.tasks",
      "destination_type": "queue",
      "routing_key": "agent.tasks.#",
      "arguments": {}
    },
    {
      "source": "openclaw",
      "vhost": "/",
      "destination": "agent.results",
      "destination_type": "queue",
      "routing_key": "agent.results.#",
      "arguments": {}
    },
    {
      "source": "openclaw",
      "vhost": "/",
      "destination": "agent.events",
      "destination_type": "queue",
      "routing_key": "agent.events.#",
      "arguments": {}
    }
  ]
}
